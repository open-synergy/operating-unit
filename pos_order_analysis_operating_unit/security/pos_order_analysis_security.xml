<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="ir_rule_pos_order_analysis_allowed_operating_units" model="ir.rule">
            <field name="model_id" ref="pos_order_analysis.model_pos_order_analysis"/>
            <field name="domain_force">['|', ('session_id.config_id.operating_unit_id','=',False), ('session_id.config_id.operating_unit_id','in',[g.id for g in user.operating_unit_ids])]</field>
            <field name="name">PoS Order Analysis from allowed operating units</field>
            <field name="global" eval="True"/>
            <field eval="0" name="perm_unlink"/>
            <field eval="0" name="perm_write"/>
            <field eval="1" name="perm_read"/>
            <field eval="0" name="perm_create"/>
        </record>
    </data>
</openerp>
